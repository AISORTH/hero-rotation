package-as: HeroRotation

externals:
 HeroLib:
  url: https://github.com/herotc/hero-lib

required-dependencies:
  - 271528-herolib

move-folders:
 hero-rotation/HeroRotation: HeroRotation
 hero-rotation/HeroRotation_DeathKnight: HeroRotation_DeathKnight
 hero-rotation/HeroRotation_DemonHunter: HeroRotation_DemonHunter
 hero-rotation/HeroRotation_Druid: HeroRotation_Druid
 hero-rotation/HeroRotation_Hunter: HeroRotation_Hunter
 hero-rotation/HeroRotation_Mage: HeroRotation_Mage
 hero-rotation/HeroRotation_Monk: HeroRotation_Monk
 hero-rotation/HeroRotation_Paladin: HeroRotation_Paladin
 hero-rotation/HeroRotation_Priest: HeroRotation_Priest
 hero-rotation/HeroRotation_Rogue: HeroRotation_Rogue
 hero-rotation/HeroRotation_Shaman: HeroRotation_Shaman
 hero-rotation/HeroRotation_Warlock: HeroRotation_Warlock
 hero-rotation/HeroRotation_Warrior: HeroRotation_Warrior
 hero-rotation/hero-lib: hero-lib
 hero-lib/HeroLib: HeroLib
 hero-lib/HeroCache: HeroCache

ignore:
 - HeroRotation_ClassTemplate/HeroRotation_Class.toc
 - HeroRotation_ClassTemplate/Class.lua
 - HeroRotation_ClassTemplate/Events.lua
 - HeroRotation_ClassTemplate/Settings.lua
 - HeroRotation_ClassTemplate/Spec.lua
 - symlink.bat
 - symlink.sh

enable-nolib-creation: no

license-output: LICENSE
